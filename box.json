{
    "directories": [
        "src/"
    ],
    "files": [
        "LICENSE",
        "README.md",
        "geggs.yml"
    ],
    "finder": [
        {
            "name": [
                "*.php",
                "*.json",
                "*.twig"
            ],
            "exclude": [
                ".gitignore",
                ".idea",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests",
                "leaphub/phpcs-symfony2-standard",
                "phpro/grumphp",
                "sensio/distribution-bundle",
                "squizlabs/php_codesniffer"
            ],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "compression": "GZ",
    "main": "bin/geggs",
    "output": "geggs.phar",
    "stub": true,
    "chmod": "0755"
}
