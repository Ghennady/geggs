{
    "name": "octava/geggs",
    "description": "Git bunch",
    "type": "project",
    "require": {
        "php": ">=5.5.0",
        "herrera-io/phar-update": "~2.0",
        "kherge/amend": "^3.0",
        "symfony/console": "^2.8",
        "symfony/process": "^2.8",
        "symfony/config": "^2.8",
        "symfony/dependency-injection": "^2.8",
        "symfony/yaml": "^2.8"
    },
    "require-dev": {
        "symfony/var-dumper": "^2.8",
        "phpro/grumphp": "^0.7.0",
        "leaphub/phpcs-symfony2-standard": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Pak",
            "email": "TheRatW@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [
            "app/AppKernel.php"
        ]
    },
    "bin": [
        "app/geggs"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
